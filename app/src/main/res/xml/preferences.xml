<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ListPreference
        android:key="country"
        android:title="Country code"/>

    <ListPreference
        android:title="Notifications Visibility"
        android:key="notification_visibility"
        android:entries="@array/pref_notification_visibility"
        android:entryValues="@array/pref_notification_visibility_values"
        android:defaultValue="PUBLIC" />

</PreferenceScreen>